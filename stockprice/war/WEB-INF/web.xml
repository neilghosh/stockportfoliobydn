<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5">
	<servlet>
		<servlet-name>stockprice</servlet-name>
		<servlet-class>ng.StockPriceServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>stockname</servlet-name>
		<servlet-class>ng.StockNameServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>EnterStockSymbolServlet</servlet-name>
		<servlet-class>ng.EnterStockSymbolServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>stockname</servlet-name>
		<url-pattern>/stockname</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>stockprice</servlet-name>
		<url-pattern>/stockprice</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>EnterStockSymbolServlet</servlet-name>
		<url-pattern>/sign</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
